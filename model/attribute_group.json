{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "http://product-api.europeansourcing.com/schemas/attribute_group.json",
  "title": "Attribute Group",
  "description": "Describe an attribute group object",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "description": "Unique id of the attribute group"
    },
    "name": {
      "type": "string",
      "description": "Name of the attribute group"
    },
    "slug": {
      "type": "string",
      "description": "Unique slug of the attribute group's name"
    }
  }
}
